{erl_opts, []}.

{deps, [
  {lfe, ".*", {git, "https://github.com/rvirding/lfe.git", {tag, "v0.10.0"}}},
  {clj, ".*", {git, "https://github.com/lfex/clj.git", {tag, "0.3.0"}}}
  ]}.

{plugins, [
  {'lfe-compile', ".*",
    {git, "https://github.com/lfe-rebar3/compile.git", {tag, "0.2.0"}}}
  ]}.

{provider_hooks, [
  {pre, [{compile, {lfe, compile}}]}
  ]}.
